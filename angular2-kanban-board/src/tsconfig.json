{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types",
      "./app/common-kanban-redux/*",
      "./app/common-kanban-redux/reducers/*",
      "./app/common-kanban-redux/actions/*"
    ]
  },
  "paths": {
    "*": [
      "./app/common-kanban-redux/*",
      "./app/common-kanban-redux/reducers/*",
      "./app/common-kanban-redux/actions/*"
    ]
  }
}
